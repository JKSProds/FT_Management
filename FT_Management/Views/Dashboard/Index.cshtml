@model IEnumerable<FT_Management.Models.Marcacao>

@{
    ViewData["Title"] = "Dashboard";
}


<div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel" data-interval="15000">
    <div class="carousel-inner">
        <div class="carousel-item active col-md-12">
            @await Component.InvokeAsync("PedidosDiarios", new { model = Model })
        </div>
        <div class="carousel-item col-md-12">
            @await Component.InvokeAsync("PedidoOrcamento", new { model = Model })
        </div>
        <div class="carousel-item col-md-12">
            @await Component.InvokeAsync("PedidoPecas", new { model = Model })
        </div>
        <div class="carousel-item col-md-12">
            @await Component.InvokeAsync("AguardarCliente", new { model = Model })
        </div>
        <div class="carousel-item col-md-12">
            @await Component.InvokeAsync("Oficina", new { model = Model })
        </div>
    </div>

    <a class="carousel-control-prev" href="#carouselExampleSlidesOnly" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleSlidesOnly" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>


